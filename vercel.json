{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "claude/vin-diesel-setup-019h3xff9zaUDiNfj1xd2nct": true
    }
  },
  "headers": [
    {
      "source": "/(.*)",
